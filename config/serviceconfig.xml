		</authorizationSocket>
		<!-- Example: NA -->
		<authorizationSocket disabled="true">
			<listen ip="0.0.0.0" port="2106" />
			<service host="64.25.35.104" port="2106" />
		</authorizationSocket>
	</authorizationSockets>

